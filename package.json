{
  "name": "eminent",
  "version": "0.0.8",
  "description": "A DOM assertions library with Emmet syntax for JavaScript testing",
  "main": "lib/eminent.js",
  "scripts": {
    "lint": "eslint src/*.js",
    "test": "npm run lint && nyc mocha --compilers js:babel-core/register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel src/ -d lib/",
    "docs": "jsdoc2md --template jsdoc2md/README.hbs lib/eminent.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathancahill/eminent.git"
  },
  "keywords": [
    "testing",
    "assert",
    "assertion",
    "test",
    "dom",
    "emmet"
  ],
  "author": "Nathan Cahill <nathan@nathancahill.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathancahill/eminent/issues"
  },
  "engines": {
    "node": ">=4"
  },
  "homepage": "https://github.com/nathancahill/eminent#readme",
  "devDependencies": {
    "babel": "^6.0.12",
    "babel-cli": "^6.0.14",
    "babel-core": "^6.0.14",
    "babel-eslint": "^4.1.3",
    "babel-preset-es2015": "^6.0.14",
    "coveralls": "^2.11.4",
    "eslint": "^1.8.0",
    "mocha": "^2.3.3",
    "nyc": "^3.2.2"
  },
  "dependencies": {
    "dom-whitespace": "^0.1.2",
    "emmet": "^1.3.1",
    "jsdom": "^7.0.2"
  }
}
